{
  "name": "angular2-bootstrap-pagination",
  "version": "2.1.3",
  "description": "angular2-bootstrap-pagination is angular2 directive. It will generate pagination or this directive must need boostrap package",
  "main": "index.js",
  "scripts": {
    "start": "$(npm bin)/tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "$(npm bin)/tsc",
    "tsc:w": "$(npm bin)/tsc -w",
    "lite": "lite-server",
    "aot": "ngc -p tsconfig-aot.json",
    "typings": "$(npm bin)/typings",
    "postinstall": "$(npm bin)/typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajan-g/angular2-bootstrap-pagination.git"
  },
  "dependencies": {
    
  },
  "devDependencies": {
    "@angular/common": "~5.0.0",
    "@angular/compiler": "~5.0.0",
    "@angular/compiler-cli": "~5.0.0",
    "@angular/core": "~5.0.0",
    "@angular/forms": "~5.0.0",
    "@angular/http": "~5.0.0",
    "@angular/platform-browser": "~5.0.0",
    "@angular/platform-browser-dynamic": "~5.0.0",
    "@angular/platform-server": "~5.0.0",
    "@angular/router": "~5.0.0",
    "@angular/upgrade": "~5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^6.0.45",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.5.2",
    "systemjs": "0.19.39",
    "typescript": "2.4.2",
    "zone.js": "^0.8.18",
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typings": "2.1.1"
  },
  "keywords": [
    "angular2-bootstrap-pagination",
    "ng2-bootstrap-pagination",
     "ng2-pagination",
    "bootstrap-pagination"
  ],
  "author": "Rajan G",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rajan-g/angular2-bootstrap-pagination.git/issues"
  },
  "homepage": "https://github.com/rajan-g/angular2-bootstrap-pagination.git#readme"
}
